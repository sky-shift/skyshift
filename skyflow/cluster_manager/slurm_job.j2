{
    "script": "{{submission_script}}",
    "job": {
      "name": "{{name}}",
      "account": "{{account}}",
      "hold": false,
      "environment": {
        "PATH": "{{path}}",
        "HOME": "{{home}}",
        "XDG_RUNTIME_DIR": "/run/user/1000"
      },
      "tasks": {{cpus}},
      "memory_per_cpu": {{memory_per_cpu}},
      "time_limit": {{time_limit}}
    }
  }
  